    "@pulumi/synced-folder": "^0.11.1",
    "@types/node": "^20.9.1",
    "ts-node": "^10.9.1"
  },
  "resolutions": {
    "@types/node": "^20.9.1",
    "typescript": "^5.2.2",
    "ts-node": "^10.9.1"
  }
}
