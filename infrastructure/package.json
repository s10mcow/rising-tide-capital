{
  "name": "react-material-starter-v2-infrastructure",
  "version": "1.0.0",
  "description": "Pulumi infrastructure for react-material-starter-v2",
  "main": "index.ts",
  "scripts": {
    "pulumi": "NODE_OPTIONS='--loader ts-node/esm' pulumi",
