{
  "name": "react-material-starter-v2",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "prepare": "husky install"
  },
  "release-it": {
    "git": {
      "commitMessage": "chore: release v${version}",
      "pushRepo": "origin",
      "requireBranch": "master",
      "requireCommits": true
    },
    "github": {
      "release": true
    },
    "npm": {
      "publish": false
    },
    "plugins": {
      "@release-it/conventional-changelog": {
        "preset": "angular",
        "infile": "CHANGELOG.md"
      }
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "dependencies": {
    "@emotion/react": "^11.11.0",
    "@emotion/styled": "^11.11.0",
    "@firebase/app": "^0.9.27",
    "@firebase/auth": "^1.6.0",
    "@googlemaps/js-api-loader": "^1.16.2",
    "@hookform/resolvers": "^3.2.0",
    "@mui/base": "^5.0.0-beta.10",
    "@mui/icons-material": "^5.14.3",
    "@mui/lab": "^5.0.0-alpha.139",
    "@mui/material": "^5.14.4",
    "@mui/styled-engine": "npm:@mui/styled-engine-sc@^5.0.0",
    "@mui/styles": "^5.14.4",
    "@mui/system": "^5.14.4",
    "@mui/x-data-grid-pro": "^6.11.2",
    "@mui/x-date-pickers": "^6.11.0",
    "@mui/x-license-pro": "^6.10.2",
    "@react-google-maps/api": "^2.19.3",
    "@sentry/react": "^7.74.1",
    "@sentry/tracing": "^7.74.1",
    "@tanstack/react-query": "^5.17.1",
    "@thesparklaboratory/form": "0.0.13",
    "@thesparklaboratory/teetimeportal-react-query-client": "1.0.2",
    "autosuggest-highlight": "^3.3.4",
    "axios": "^1.5.1",
    "date-fns": "^2.30.0",
    "eslint-plugin-react": "^7.26.1",
    "firebase": "^10.8.0",
    "notistack": "^3.0.1",
    "qs": "^6.11.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.45.4",
    "react-router-dom": "^6.17.0",
    "recoil": "^0.7.7",
    "recoil-nexus": "^0.5.0",
    "styled-components": "^6.0.0-rc.1",
    "yup": "^1.3.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.7.1",
    "@commitlint/config-conventional": "^17.7.0",
    "@pulumi/aws": "^6.13.1",
    "@pulumi/synced-folder": "^0.11.1",
    "@tanstack/react-query-devtools": "^5.0.0",
    "@thesparklaboratory/eslint-config-react": "^0.0.1",
    "@thesparklaboratory/tsconfig-react": "^0.0.1",
    "@thesparklaboratory/vite-config-react": "^0.0.3n",
    "@types/autosuggest-highlight": "^3.2.3",
    "@types/lodash": "^4.14.202",
    "@types/qs": "^6.9.11",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@types/styled-components": "^5.1.26",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.0.4",
    "eslint": "^8.45.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "eslint-plugin-unused-imports": "^3.0.0",
    "husky": "^8.0.0",
    "jsdom": "^22.1.0",
    "lint-staged": "^13.2.2",
    "prettier": "^3.0.3",
    "pretty-quick": "^3.1.3",
    "typescript": "^5.0.2",
    "vite": "^4.4.9",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-svgr": "^3.2.0",
    "vite-tsconfig-paths": "^4.2.0",
    "vitest": "^0.34.1"
  }
}
